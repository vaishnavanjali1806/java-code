package anjali;

import java.util.Scanner;

public class percentage {

	public static void main(String[] args) {
	
       Scanner scn = new Scanner(System.in);
       int a= scn.nextInt();
       int b= scn.nextInt();
       int c= scn.nextInt();
       int d= scn.nextInt();
       float p = (a+b+c+d)/4;
       System.out.println("Precentage obtained:"+p);
    		  
       
       
	}

}
